<manifest
    package="com.github.marwinxxii.rxsamples"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".EditTextSampleActivity"
            android:label="@string/sample_edittext" />

        <activity
            android:name=".EmailSampleActivity"
            android:label="@string/sample_email" />

        <activity
            android:name=".wizard.WizardSampleActivity"
            android:label="@string/sample_wizard" />
    </application>

</manifest>
